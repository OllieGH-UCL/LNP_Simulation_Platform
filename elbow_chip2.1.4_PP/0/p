    {
        type            zeroGradient;
    }

    inlet_1
    {
        type            zeroGradient;
    }

    inlet_2
    {
        type            zeroGradient;
    }

    outlet
    {
        type            fixedValue;
        value           uniform 0;
    }

    wall_horizental
    {
        type            zeroGradient;
    }

    frontAndBackPlanes
    {
        type            empty;
    }
}


